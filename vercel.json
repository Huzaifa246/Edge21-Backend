{
   "version": 2,
   "builds": [
      {
         "src": "index.js",
         "use": "@vercel/node"
      },
      {
         "src": "api/scheduled-task.js",
         "use": "@vercel/node"
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "index.js"
      }
   ],
   "functions": {
      "api/scheduled-task.js": {
         "schedule": "0 * * * *"
      }
   }
}